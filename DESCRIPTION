Package: LOGAN
Title: Log File Analysis in International Large-scale Assessments
Version: 0.0.0.9007
Authors@R: c(
    person("Denise", "Reis Costa",
            email = "d.r.costa@cemo.uio.no", role = c("aut", "cre")),
    person("Waldir", "Leoncio",
            email = "waldir.leoncio@gmail.com", role = c("aut"))
